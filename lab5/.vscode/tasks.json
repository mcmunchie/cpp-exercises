{
  "tasks": [
    {
      "type": "cppbuild",
      "label": "Build with GCC 11.3.0",
      "command": "C:\\mingw64\\bin\\g++.exe",
      "args": [
        "-g",
        "-std=c++20",
        "${workspaceFolder}\\*.cpp",
        "-o",
        "${fileDirname}\\kitten.exe"
      ],
      "options": {
        "cwd": "${fileDirname}"
      },
      "problemMatcher": [
        "$gcc"
      ],
      "group": "build",
      "detail": "compiler: C:\\mingw64\\bin\\g++.exe"
    },
    {
      "type": "cppbuild",
      "label": "Build with Clang 14.0.3",
      "command": "C:\\mingw64\\bin\\clang++.exe",
      "args": [
        "-g",
        "-std=c++20",
        "${workspaceFolder}\\*.cpp",
        "-o",
        "${fileDirname}\\kitten.exe"
      ],
      "options": {
        "cwd": "${fileDirname}"
      },
      "problemMatcher": [
        "$gcc"
      ],
      "group": "build",
      "detail": "compiler: C:\\mingw64\\bin\\clang++.exe"
    },
    {
      "type": "cppbuild",
      "label": "Build with MSVC",
      "command": "cl.exe",
      "args": [
        "/Zi",
        "/std:c++latest",
        "/EHsc",
        "/Fe:",
        "${fileDirname}\\kitten.exe",
        "${workspaceFolder}\\*.cpp"
      ],
      "options": {
        "cwd": "${fileDirname}"
      },
      "problemMatcher": [
        "$msCompile"
      ],
      "group": "build",
      "detail": "compiler: cl.exe"
    }
  ],
  "version": "2.0.0"
}